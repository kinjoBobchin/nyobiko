{
  "env": {
    "browser": true
  },
  "globals": {
      "$" : false
    },
  "rules": {
    "no-extra-semi": "warn",
    "quotes": ["warn", "double"],
    "no-undef": "warn",
    "space-before-blocks": ["warn",
      {
        "functions": "always",
        "keywords": "always",
        "classes": "always"
      }
    ]
  }
}