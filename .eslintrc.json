{
  "globals": {
      "alert" : false,
      "console" : false,
      "$" : false,
      "window" : false,
      "document" : false
    },
  "rules": {
    "no-extra-semi": "warn",
    "quotes": ["warn", "double"],
    "no-undef": "warn",
    "space-before-blocks": ["warn",
      {
        "functions": "always",
        "keywords": "always",
        "classes": "always"
      }
    ]
  }
}